class Solution {
  public:
    vector<int> findMissing(vector<int>& a, vector<int>& b) {
     unordered_set<int> bSet;
        vector<int> result;

        // Step 1: Insert all elements of b into the set
        for (int i = 0; i < b.size(); i++) {
            bSet.insert(b[i]);
        }

        // Step 2: Traverse a and check which elements are missing in bSet
        for (int i = 0; i < a.size(); i++) {
            if (bSet.find(a[i]) == bSet.end()) {
                result.push_back(a[i]);
            }
        }

        return result;
    }
};
