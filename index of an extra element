class Solution {
  public:
    int findExtra(vector<int>& a, vector<int>& b) {
        int low = 0, high = b.size(); // b is shorter by 1 element

        while (low <= high) {
            int mid = low + (high - low) / 2;

            if (mid < b.size() && a[mid] == b[mid]) {
                low = mid + 1;
            } else {
                high = mid - 1;
            }
        }

        return low;
    }
};
